{
    "name":        "knplabs/knp-disqus-bundle",
    "type":        "symfony-bundle",
    "keywords":    ["comments", "forum", "disqus"],
    "homepage":    "http://github.com/KnpLabs/KnpDisqusBundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "KnpLabs Team",
            "homepage": "http://knplabs.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/KnpLabs/KnpDisqusBundle/contributors"
        }
    ],

    "require": {
        "php":                      ">=5.3.2",
        "symfony/framework-bundle": "2.1.*",
        "kriswallsmith/buzz":       "*"
    },

    "recommend": {
        "knp/zend-cache-bundle": "*"
    },

    "autoload": {
        "psr-0": {
            "Knp\\Bundle\\DisqusBundle": ""
        }
    },

    "target-dir": "Knp/Bundle/DisqusBundle",

    "repositories": {
        "knp/zend-cache-bundle": {
            "package": {
                "name": "knp/knp-zend-cache-bundle",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/KnpLabs/KnpZendCacheBundle/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/KnpZendCacheBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "zendframework/zend-cache": "*",
                    "zendframework/zend-filter": "*"
                },
                "autoload": {
                    "psr-0": { "Knp\\Bundle\\ZendCacheBundle": "" }
                },
                "target-dir": "Knp/Bundle/ZendCacheBundle"
            }
        },
        "zendframework/zend-cache": {
            "package": {
                "name": "zendframework/zend-cache",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/KnpLabs/zend-cache/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/zend-cache.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Zend\\Cache": "php/" }
                }
            }
        },
        "zendframework/zend-filter": {
            "package": {
                "name": "zendframework/zend-filter",
                "dist": {
                    "url": "https://github.com/KnpLabs/zend-filter/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/zend-filter.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Zend\\Filter": "php/" }
                }
            }
        }
    }
}
